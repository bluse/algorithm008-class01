/**
 * 350. 两个数组的交集 II
 * leetcode地址: https://leetcode-cn.com/problems/intersection-of-two-arrays-ii/
 */
//代码实现(go语言实现)
func intersect(nums1 []int, nums2 []int) []int {
    res, nums1Map := []int{}, make(map[int]int)
    for _, v := range nums1 {
        nums1Map[v]++
    }
    for _, v := range nums2 {
        nums1Map[v]--
        if nums1Map[v] >= 0 {
            res = append(res, v)
        }
    }

    return res
}
